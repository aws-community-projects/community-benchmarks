{
  "name": "community-benchmarks",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "sst build",
    "cover": "sst test --coverage",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "start": "sst start",
    "test": "sst test"
  },
  "eslintConfig": {
    "extends": [
      "serverless-stack"
    ]
  },
  "devDependencies": {
    "@serverless-stack/cli": "0.69.5",
    "@serverless-stack/resources": "0.69.5",
    "@tsconfig/node14": "^1.0.1",
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^27.4.1",
    "@types/node": "<15.0.0",
    "aws-cdk-lib": "2.15.0",
    "aws-sdk-client-mock": "^0.6.2",
    "aws-sdk-mock": "^5.6.2",
    "typesafe-dynamodb": "^0.1.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/client-lambda": "^3.65.0",
    "@aws-sdk/client-s3": "^3.58.0",
    "@aws-sdk/client-xray": "^3.58.0",
    "@aws-sdk/lib-dynamodb": "^3.58.0",
    "aws-sdk": "^2.1109.0",
    "csvtojson": "^2.0.10"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,ts}",
      "!**/.build/**",
      "!**/.sst/**",
      "!**/cdk.out/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!stacks/index.ts"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  }
}
